    <TargetFramework>$(CompilerNetFramework)</TargetFramework>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <RootNamespace>Windows</RootNamespace>
    <AssemblyName>Windows</AssemblyName>
    <OutputPath>$(NScriptToolSetPath)lib\</OutputPath>
    <NoWarn>0067; 0824; 0169; 0649; 0626; 0414; 0660; 0661; 3001; 3002</NoWarn>
    <NScriptToolSetPath>..\..\..\NScriptToolSet\</NScriptToolSetPath>
