<Project Sdk="NScript.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <RootNamespace>Sunlight.Framework</RootNamespace>
    <AssemblyName>Sunlight.Framework</AssemblyName>
    <NScriptToolSetPath>..\..\..\NScriptToolSet\</NScriptToolSetPath>
    <NScriptBinPath>$(NScriptToolSetPath)bin\netcoreapp3.1\</NScriptBinPath>
    <OutputPath>$(NScriptToolSetPath)lib\</OutputPath>
    <NoWarn>0824; 0169; 0649; 0626; 0414; 0660; 0661; 3001; 3002</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="mscorlib" Version="1.0.0" />
    <PackageReference Include="System.Web" Version="1.0.0" />
  </ItemGroup>
</Project>