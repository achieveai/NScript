<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <OutputType>Library</OutputType>
    <NoStdLib>True</NoStdLib>
    <CopyReferences>True</CopyReferences>
    <TemplateFile>Properties\Script.jst</TemplateFile>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <RootNamespace>RealScript.Debug</RootNamespace>
    <AssemblyName>RealScript.Debug</AssemblyName>
    <BaseIntermediateOutputPath>$(ProjectDir)obj</BaseIntermediateOutputPath>
    <NScriptToolSetPath>..\..\..\NScriptToolSet</NScriptToolSetPath>
    <CscToolExe>$(NScriptToolsetDir)\bin\$(CompilerNetFramework)\csc.exe</CscToolExe>
    <OutputPath>$(ProjectDir)..\bin\</OutputPath>
    <NoWarn>0028, 1591, 0414, 0219, 0824, 0626, 0169, 0168</NoWarn>
    <ResolveNuGetPackages>false</ResolveNuGetPackages>
    <DisableImplicitFrameworkReferences>true</DisableImplicitFrameworkReferences>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DefineConstants>DEBUG</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <CodeAnalysisRuleSet>AllRules.ruleset</CodeAnalysisRuleSet>
    <GenerateSerializationAssemblies>Off</GenerateSerializationAssemblies>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DefineConstants>DEBUG</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <CodeAnalysisRuleSet>AllRules.ruleset</CodeAnalysisRuleSet>
    <Optimize>false</Optimize>
    <DebugType>full</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>
  <ItemGroup>
    <None Include="RealScript.il" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../../../Sources/Framework/mscorlib/NScript.MsCorlib.csproj" />
    <ProjectReference Include="../../../Sources/Framework/System.Core/System.Core.csproj" />
    <ProjectReference Include="../../../Sources/Framework/Microsoft.CSharp/Microsoft.CSharp.csproj" />
  </ItemGroup>
  <!--
  <Import Project="$(ProgramFiles)\nStuff\ScriptSharp\v1.0\nStuff.ScriptSharp.targets" />
  -->
  <!--<Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />-->
</Project>