<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>$(CompilerNetFramework)</TargetFramework>
    <Platform Condition=" '$(Platform)' == '' ">x86</Platform>
    <OutputType>Exe</OutputType>
    <AssemblyName>csc</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <LangVersion>latest</LangVersion>
    <StartupObject>JsCsc.Program</StartupObject>
    <RunAnalyzersDuringBuild>true</RunAnalyzersDuringBuild>
    <EnableNETAnalyzers>false</EnableNETAnalyzers>
    <Prefer32Bit>true</Prefer32Bit>
    <OutputPath>$(NScriptToolsetDir)\bin\</OutputPath>
    <ProduceReferenceAssemblies>false</ProduceReferenceAssemblies>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\NScript.Csc.Lib\NScript.Csc.Lib.csproj" />
  </ItemGroup>
</Project>
