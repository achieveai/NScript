<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>$(FrameworkTFM)</TargetFramework>
    <OutputType>Library</OutputType>
    <RootNamespace>System.Web.Html</RootNamespace>
    <AssemblyName>System.Web.Html</AssemblyName>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <NoStdLib>true</NoStdLib>
    <NoWarn>0824; 0169; 0649; 0626; 0444; 0414; 0660; 0661</NoWarn>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <DisableImplicitFrameworkReferences>true</DisableImplicitFrameworkReferences>
    <!-- <OutputPath>$(NScriptToolSetPath)lib\</OutputPath> -->
    <!-- <CscToolPath>$(NScriptToolsetDir)\bin\$(CompilerNetFramework)\</CscToolPath> -->
    <CscToolExe>$(NScriptToolsetDir)\bin\$(CompilerNetFramework)\csc.exe</CscToolExe>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="../mscorlib/NScript.MsCorlib.csproj" />
    <ProjectReference Include="../System.Core/System.Core.csproj" />
    <ProjectReference Include="../System.Web/System.Web.csproj" />
    <ProjectReference Include="../Microsoft.CSharp/Microsoft.CSharp.csproj" />
  </ItemGroup>
</Project>