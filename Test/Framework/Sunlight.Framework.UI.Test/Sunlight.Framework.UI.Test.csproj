<Project Sdk="NScript.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <RootNamespace>Sunlight.Framework.UI.Test</RootNamespace>
    <AssemblyName>Sunlight.Framework.UI.Test</AssemblyName>
    <NScriptToolSetPath>..\..\..\NScriptToolSet</NScriptToolSetPath>
    <NScriptBinPath>$(NScriptToolSetPath)\bin\$(CompilerNetFramework)\</NScriptBinPath>
    <OutputPath>$(NScriptToolSetPath)lib\</OutputPath>
    <GenerateJs>True</GenerateJs>
    <JsOutputPath>..\TestWebApplication\GeneratedScripts</JsOutputPath>
    <NoWarn>0824</NoWarn>
    <PluginConfig>PluginConfig.xml</PluginConfig>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\..\sources\Framework\mscorlib\NScript.MsCorlib.csproj" />
    <ProjectReference Include="..\..\..\Sources\Framework\Sunlight.Framework.UI\Sunlight.Framework.UI.csproj" />
    <ProjectReference Include="..\..\..\sources\Framework\Sunlight.Framework\Sunlight.Framework.csproj" />
    <ProjectReference Include="..\..\..\sources\Framework\Sunlight.Framework\Sunlight.Framework.UI.csproj" />
    <ProjectReference Include="..\..\..\Sources\Framework\System.Web.Html\System.Web.Html.csproj" />
    <ProjectReference Include="..\..\..\Sources\Framework\System.Web\System.Web.csproj" />
    <ProjectReference Include="..\..\..\Sources\Framework\SunlightUnit\SunlightUnit.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="PluginConfig.xml" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Templates\TestTemplate1.html" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Templates\TestTemplateVMB1.html" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Templates\TestTemplateVMB_AttrBinding.html" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Templates\TestTemplateVMB_CssBinding.html" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Templates\TestTemplateVMB_StyleBinding.html" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Templates\TestTemplateB_PropertyBinding.html" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Templates\TestTemplateVMC1.html" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Templates\TestTemplateVMB1_int.html" />
  </ItemGroup>
  <PropertyGroup>
    <PostBuildEvent>copy *.js ..\..\..\TestWebApplication\GeneratedScripts
		copy *.map ..\..\..\TestWebApplication\GeneratedScripts
		copy *.ashx ..\..\..\TestWebApplication\GeneratedScripts
	</PostBuildEvent>
  </PropertyGroup>
</Project>
