<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>$(FrameworkTFM)</TargetFramework>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <RootNamespace>Sunlight.Framework.UI</RootNamespace>
    <AssemblyName>Sunlight.Framework.UI</AssemblyName>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <DisableImplicitFrameworkReferences>true</DisableImplicitFrameworkReferences>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="../mscorlib/NScript.MsCorlib.csproj" />
    <ProjectReference Include="../Sunlight.Framework/Sunlight.Framework.csproj" />
    <ProjectReference Include="../System.Web.Html/System.Web.Html.csproj" />
  </ItemGroup>
</Project>