<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>$(FrameworkTFM)</TargetFramework>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <RootNamespace>Sunlight.Framework</RootNamespace>
    <AssemblyName>Sunlight.Framework</AssemblyName>
    <NoWarn>0824; 0169; 0649; 0626; 0414; 0660; 0661; 3001; 3002</NoWarn>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <DisableImplicitFrameworkReferences>true</DisableImplicitFrameworkReferences>
    <CscToolPath>$(NScriptToolsetDir)\bin\$(CompilerNetFramework)\</CscToolPath>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="../mscorlib/NScript.MsCorlib.csproj" />
    <ProjectReference Include="../System.Web/System.Web.csproj" />
  </ItemGroup>
</Project>